    "@types/react-router-dom": "^4.3.4",
    "@types/styled-components": "^4.1.18",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-plugin-react": "^7.16.0",
    "eslint-plugin-react-hooks": "^2.2.0",
    "prettier": "^1.18.2",
    "react-scripts": "3.1.2",
    "typescript": "3.5.3"
